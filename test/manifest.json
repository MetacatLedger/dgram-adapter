{
  "manifest_version": 2,
  "version": "0.1",
  "name": "node-dgram@libdweb",
  "description": "Node drgam test",
  "permissions": ["dns"],

  "experiment_apis": {
    "UDPSocket": {
      "schema": "../node_modules/libdweb/src/UDPSocket/UDPSocket.json",
      "child": {
        "scopes": ["addon_child"],
        "paths": [["UDPSocket"]],
        "script": "../node_modules/libdweb/src/UDPSocket/Socket.js"
      }
    }
  },
  "background": {
    "scripts": ["test.js"]
  }
}
